{{template "base/head" .}}
<div
	role="main"
	aria-label="{{.Title}}"
	class="page-content organization teams"
>
	<script>
		window.config.pageData = {
			...window.config.pageData ,
			isTeamReposPage:{{.PageIsOrgTeamRepos}},
			teamName:{{.Title}},
			orgLink:{{.OrgLink}},
			team: {{ .Team }},
			mode: "edit",
			customPrivilegesUnits: {{ .CustomPrivilegesUnits }}
		};
	</script>
	{{template "org/header" .}}
	<div class="container ui">
		{{template "base/alert" .}}
		<div id="v-app-team-privileges"></div>
	</div>
</div>

{{template "base/footer" .}}
