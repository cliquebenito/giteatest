<!DOCTYPE html>
<html lang="{{.locale.Lang}}" class="override theme-{{if .SignedUser.Theme}}{{.SignedUser.Theme}}{{else}}{{DefaultTheme}}{{end}}">
<head>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>{{if .Title}}{{.Title | RenderEmojiPlain}} - {{end}}{{if .Repository.Name}}{{.Repository.Name}} - {{end}} SourceControl</title>
	{{if .ManifestData}}<link rel="manifest" href="data:{{.ManifestData}}">{{end}}
	{{if ThemeColorMetaTag}}<meta name="theme-color" content="{{ThemeColorMetaTag}}">{{end}}
	<meta name="default-theme" content="{{DefaultTheme}}">
	<meta name="author" content="{{if .Repository}}{{.Owner.Name}}{{else}}SourceControl{{end}}">
	<meta name="description" content="{{if .Repository}}{{.Repository.Name}}{{if .Repository.Description}} - {{.Repository.Description}}{{end}}{{else}}SourceControl{{end}}">
	<meta name="keywords" content="SourceControl">
	<meta name="referrer" content="no-referrer">
	{{if .GoGetImport}}
		<meta name="go-import" content="{{.GoGetImport}} git {{.RepoCloneLink.HTTPS}}">
		<meta name="go-source" content="{{.GoGetImport}} _ {{.GoDocDirectory}} {{.GoDocFile}}">
	{{end}}
	{{if and .EnableFeed .FeedURL}}
		<link rel="alternate" type="application/atom+xml" title="" href="{{.FeedURL}}.atom">
		<link rel="alternate" type="application/rss+xml" title="" href="{{.FeedURL}}.rss">
	{{end}}
	<link rel="stylesheet" href="{{AssetUrlPrefix}}/css/index.css?v={{AssetVersion}}">
	{{if .OneWork.Enabled}}
		<link rel="preload" href="{{AssetUrlPrefix}}/css/side-menu.css?v={{AssetVersion}}" as="style">
		<link rel="stylesheet" href="{{AssetUrlPrefix}}/css/side-menu.css?v={{AssetVersion}}">
	{{end}}
	<link rel="icon" href="{{AssetUrlPrefix}}/img/favicon.svg" type="image/svg+xml">
  <link rel="alternate icon" href="{{AssetUrlPrefix}}/img/favicon.ico" type="image/x-icon">
  <link rel="apple-touch-icon" sizes="180x180" href="{{AssetUrlPrefix}}/img/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="{{AssetUrlPrefix}}/img/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="{{AssetUrlPrefix}}/img/favicon-16x16.png">
	{{template "base/head_script" .}}

	<noscript>
		<style>
			.dropdown:hover > .menu { display: block; }
			.ui.secondary.menu .dropdown.item > .menu { margin-top: 0; }
		</style>
	</noscript>
	{{if .PageIsUserProfile}}
		<meta property="og:title" content="{{.ContextUser.DisplayName}}">
		<meta property="og:type" content="profile">
		<meta property="og:image" content="{{.ContextUser.AvatarLink $.Context}}">
		<meta property="og:url" content="{{.ContextUser.HTMLURL}}">
		{{if .ContextUser.Description}}
			<meta property="og:description" content="{{.ContextUser.Description}}">
		{{end}}
	{{else if .Repository}}
		{{if .Issue}}
			<meta property="og:title" content="{{.Issue.Title}}">
			<meta property="og:url" content="{{.Issue.HTMLURL}}">
			{{if .Issue.Content}}
				<meta property="og:description" content="{{.Issue.Content}}">
			{{end}}
		{{else}}
			<meta property="og:title" content="{{.Repository.Name}}">
			<meta property="og:url" content="{{.Repository.HTMLURL}}">
			{{if .Repository.Description}}
				<meta property="og:description" content="{{.Repository.Description}}">
			{{end}}
		{{end}}
		<meta property="og:type" content="object">
		{{if (.Repository.AvatarLink $.Context)}}
			<meta property="og:image" content="{{.Repository.AvatarLink $.Context}}">
		{{else}}
			<meta property="og:image" content="{{.Repository.Owner.AvatarLink $.Context}}">
		{{end}}
		{{else}}
			<meta property="og:title" content="SourceControl">
			<meta property="og:type" content="website">
			<meta property="og:image" content="{{AssetUrlPrefix}}/img/logo.png">
			<meta property="og:url" content="{{AppUrl}}">
			<meta property="og:description" content="SourceControl">
	{{end}}
	<meta property="og:site_name" content="SourceControl">
	{{if .IsSigned}}
		{{if ne .SignedUser.Theme "gitea"}}
			<link rel="stylesheet" href="{{AssetUrlPrefix}}/css/theme-{{.SignedUser.Theme | PathEscape}}.css?v={{AssetVersion}}">
			<link rel="stylesheet" href="{{AssetUrlPrefix}}/css/theme-overrides-{{.SignedUser.Theme | PathEscape}}.css?v={{AssetVersion}}">
		{{end}}
	{{else if ne DefaultTheme "gitea"}}
		<link rel="stylesheet" href="{{AssetUrlPrefix}}/css/theme-{{DefaultTheme | PathEscape}}.css?v={{AssetVersion}}">
		<link rel="stylesheet" href="{{AssetUrlPrefix}}/css/theme-overrides-{{DefaultTheme | PathEscape}}.css?v={{AssetVersion}}">
	{{end}}
	{{ if and EnableWidgetSW .IsSigned}}
		<script>
			window.globalConfigFeedBack = {
				getLogin: () => {{.SignedUser.LowerName}},
				getSystem: () => {{ WidgetSWSetting.SystemName }},
				getWidgetUrl: () => {{ WidgetSWSetting.ApiHostUrl }},
				getDelayBetweenRetry: () => [10000, 20000],
				getWidgetTheme: () => window.config.theme
			}
		</script>
	{{ end }}
	{{template "custom/header" .}}
</head>
<body class="{{if and .IsSigned .OneWork.Enabled}}side-menu-mode{{end}}">
	{{template "custom/body_outer_pre" .}}


	<div class="full height">
		<noscript>{{.locale.Tr "enable_javascript"}}</noscript>

		{{template "custom/body_inner_pre" .}}
		{{if and (not .PageIsInstall) .IsSigned}}
			{{if .OneWork.Enabled}}
				{{if IsWorksMode .XSSDMode}}
					{{template "base/head_navbar_ow" .}}
				{{end}}
			{{else}}
				<div class="ui top secondary stackable main menu following bar light">{{template "base/head_navbar" .}}</div>
			{{end}}
		{{end}}

		{{ if and EnableWidgetSW .IsSigned}}
			<div id="feedback-application"></div>
			<script src="{{WidgetSWSetting.DepsHostUrl}}/js/swwidget.runtime-main.js"></script>
			<script src="{{WidgetSWSetting.DepsHostUrl}}/js/swwidget.chunk.js"></script>
			<script src="{{WidgetSWSetting.DepsHostUrl}}/js/swwidget.main.chunk.js"></script>
		{{ end }}

{{if false}}
	{{/* to make html structure "likely" complete to prevent IDE warnings */}}
	</div>
</body>
</html>
{{end}}
